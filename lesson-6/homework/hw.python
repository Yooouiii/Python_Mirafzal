def modify_string(txt):
    res, c = "", 0
    for i, ch in enumerate(txt):
        res += ch; c += 1
        if c == 3 and ch not in "aeiou" and (i+1 == len(txt) or txt[i+1] != "_"):
            res += "_"; c = 0
        elif c == 3: c = 2
    return res

print(modify_string("hello"))
print(modify_string("assalom"))
print(modify_string("abcabcabcdeabcdefabcdefg"))

