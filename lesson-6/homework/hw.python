def uncommon(l1,l2):
    return [x for x in l1 if x not in l2]+[y for y in l2 if y not in l1]

print(uncommon([1,1,2],[2,3,4]))
print(uncommon([1,2,3],[4,5,6]))
print(uncommon([1,1,2,3,4,2],[1,3,4,5]))
